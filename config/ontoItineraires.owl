# ---- Classes definition ----
iti:Waypoint a owl:class;
	rdfs:label "point de cheminement"@fr, "waypoint"@en;
	rdfs:comment "classe pour définir un lieu de passage dans un itinéraire"@fr;
	rdfs:isDefinedBy iti:Waypoint.
	
iti:Landmark a owl:class;
	rdfs:label "point de vue"@fr, "landmark"@en;
	rdfs:comment "Classe pour définir un point de vue dans un itinéraire"@fr;
	rdfs:isDefinedBy iti:Landmark.
    
iti:Route a owl:Class;
    rdfs:comment "Liste ordonnée des points de cheminement."@fr, "Ordered list of waypoints."@en;
    rdfs:label "List of waypoints"@en, "Liste des points de cheminements"@fr;
    rdfs:isDefinedBy iti:Route;
    rdfs:subClassOf 
        [ 
            a owl:Class;
            owl:intersectionOf 
            (
                rdf:List
                [ 
                    a owl:Restriction;
                    owl:allValuesFrom iti:Waypoint;
                    owl:onProperty rdf:first
                ]
                                                                                                           
                [ 
                    a owl:Restriction;
                    owl:allValuesFrom iti:Route;
                    owl:onProperty rdf:rest
                ]
            )
        ] .
    

# ---- Properties definition ----
iti:departureDate a owl:ObjectProperty;
    rdfs:label "date de départ de"@fr, "departure date of"@en;
    rdfs:comment "Propriété pour définir la date de départ de quelque part."@fr;
    rdfs:isDefinedBy iti:departureDate;
	rdfs:subPropertyOf transit:departureTime;
    rdfs:domain iti:Waypoint;
    rdfs:range owl-time:TemporalEntity.

iti:arrivalDate a owl:ObjectProperty;
    rdfs:label "date d'arrivée de"@fr, "arrival date of"@en;
    rdfs:comment "Propriété pour définir la date d'arrivée quelque part."@fr;
    rdfs:isDefinedBy iti:arrivalDate;
	rdfs:subPropertyOf transit:arrivalTime;
    rdfs:domain iti:Waypoint;
    rdfs:range owl-time:TemporalEntity.

iti:spatialReference a owl:ObjectProperty;
    rdfs:label "lieu attaché au point de cheminement"@fr, "place attached to the waypoint"@en;
    rdfs:comment "Propriété pour définir le lieu attaché au point de cheminement."@fr;
    rdfs:isDefinedBy iti:spatialReference;
    rdfs:domain iti:Waypoint.
    
iti:near a owl:ObjectProperty;
    rdfs:label "proche de"@fr, "near of"@en;
    rdfs:comment "Propriété pour définir qu'un point de cheminement est proche d'un point remarquable."@fr;
    rdfs:isDefinedBy iti:near;
    rdfs:domain iti:Waypoint;
    rdfs:range owl-time:Landmark.
    
iti:waypoints a owl:ObjectProperty;
    rdfs:label "liste des points de cheminement"@fr, "list of waypoints"@en;
    rdfs:comment "Liste des points de cheminement."@fr;
    rdfs:domain iti:Route;
    rdfs:range rdf:List.